<!DOCTYPE html>
<html lang="pt-br">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Dados Escolares</title>
  <style>
    body {
      font-family: Arial, sans-serif;
    }
    .container {
      max-width: 400px;
      width: 100%;
      margin: 0 auto;
    }
  </style>
</head>
<body>
  <div class="container">
    <h2>Dados Encontrados:</h2>
    <ul>
      <% rows.forEach(function(row) { %>
        <li>
          <strong>Taxa de Aprovação:</strong> <%= row.taxa_aprovacao %><br>
          <strong>Indicador de Rendimento:</strong> <%= row.indicador_rendimento %><br>
          <strong>Nota Saeb Matemática:</strong> <%= row.nota_saeb_matematica %><br>
          <strong>Nota Saeb Língua Portuguesa:</strong> <%= row.nota_saeb_lingua_portuguesa %><br>
          <strong>Nota Saeb Média Padronizada:</strong> <%= row.nota_saeb_media_padronizada %><br>
          <strong>IDEB:</strong> <%= row.ideb %><br>
          <strong>Projeção:</strong> <%= row.projecao %><br>
        </li>
      <% }); %>
    </ul>
  </div>
</body>
</html>